<DonacionesScreen>:
    MDScreen:
        name: 'paciente_donaciones'

        MDFloatLayout:
            MDLabel:
                text: "Mis Donaciones"
                font_size: '15sp'
                font_name: 'asset/Fuentes/Poppins-SemiBold.ttf'
                pos_hint: {'center_x': .5, 'center_y': .8}
                size_hint: (.5, .2)
                halign: "center"
        AnchorLayout:
            id: donaciones
            anchor_x: "center"
            anchor_y: "center"
            pos_hint: {'center_x': .5, 'center_y': .5}