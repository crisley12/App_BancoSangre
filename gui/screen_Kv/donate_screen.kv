                            size_hint_x: .8
                            pos_hint: {"center_x": .55, "center_y": .55}
                            theme_text_color: "Custom"
                        MDFillRoundFlatButton:
                            text: "Si"
                            size_hint_x: .3
                            pos_hint: {"center_x": .35, "center_y": .45}
                            text_color: "white"
                            md_bg_color: get_color_from_hex("#F41F05")
