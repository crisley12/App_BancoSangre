                            padding: [10]
                            on_release: app.root.current = 'paciente_basantranfs'
                        MDFillRoundFlatButton:
                            text: "No"
                            size_hint_x: .3
                            pos_hint: {"center_x": .7, "center_y": .45}
                            text_color: "white"
                            md_bg_color: get_color_from_hex("#F41F05")
                            elevation: None
