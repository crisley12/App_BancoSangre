<DonacionesScreen>:
    MDScreen:
        name: 'paciente_donaciones'

        MDFloatLayout:
            MDLabel:
                text: "Mis Donaciones"
                font_size: '20sp'
                font_name: 'asset/Fuentes/Poppins-SemiBold.ttf'
                pos_hint: {'center_x': .5, 'center_y': .83}
                size_hint: (.5, .2)
                halign: "center"
        AnchorLayout:
            id: donaciones
            anchor_x: "center"
            anchor_y: "center"
            pos_hint: {'center_x': .5, 'center_y': .5}

        MDIconButton:
            icon: "arrow-left"
            pos_hint: {"center_x": .10, "center_y": .1}
            on_release:
                root.manager.transition.direction = "right" 
                app.root.current = 'root'